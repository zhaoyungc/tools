{
  "name": "tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "element-ui": "^2.15.14",
    "vue": "^2.6.6",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "json-editor-vue": "^0.9.2",
    "node-sass": "^9.0.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "uglify-js": "^3.17.4",
    "vue-lazyload": "^3.0.0-rc.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true,
      "jquery": true,
      "amd": true,
      "commonjs": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4
      ],
      "quotes": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-unreachable": "off",
      "no-redeclare": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
